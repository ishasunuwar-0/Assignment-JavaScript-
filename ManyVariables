<head><title>Many Variables</title></head>
<body>
    <script>
        let var1 = "Hello";
        let var2 = 10;
        let var3 = true;
        let var4 = null;
        let var5;
        document.write(var1 + "<br>" + var2 + "<br>" + var3 + "<br>" + var4 + "<br>" + var5);
    </script>
</body>
